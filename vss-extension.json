{
  "manifestVersion": 1,
  "id": "USITSupport-CreatePullRequest",
  "version": "1.0.0",
  "name": "USITSupport-CreatePullRequest",
  "description": "Automatically create a Pull Request for Azure DevOps from Build or Release pipeline, supports also multi target branch.",
  "publisher": "USITSupport-CreatePullRequest",
  "repository": {
    "type": "git",
    "url": "https://dev.azure.com/usitsupport/pipelines/_git/extensions"
  },
  "content": {
    "details": {
      "path": "README.md"
    },
    "license": {
      "path": "LICENSE.md"
    },
    "privacy": {
      "path": "PRIVACY.md"
    }
  },
  "links": {
    "support": {
      "uri": "https://dev.azure.com/usitsupport/pipelines/_wiki/wikis"
    },
    "license": {
      "uri": "https://dev.azure.com/usitsupport/pipelines/_git/extensions?path=/LICENSE.md"
    },
    "privacypolicy": {
      "uri": "https://dev.azure.com/usitsupport/pipelines/_git/extensions?path=/PRIVACY.md"
    }
  },
  "tags": ["Azure Pipelines", "Pull Request", "Git", "Azure Repos", "PR", "Python"],
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "icons": {
    "default": "pull.png"
  },
  "categories": ["Azure Pipelines"],
  "contributions": [
    {
      "id": "USITSupport-CreatePullRequest",
      "type": "ms.vss-distributed-task.task",
      "targets": ["ms.vss-distributed-task.tasks"],
      "properties": {
        "name": "task"
      }
    }
  ],
  "files": [
    {
      "path": "task"
    }
  ]
}
